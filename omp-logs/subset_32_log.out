  }

  // checking results
  errors = 0;
  for (int dev = 0; dev < num_dev; ++dev) {
    sum[dev] = h_matrix[dev][0];
    for (int i = 1; i < N; ++i)
      sum[dev] += h_matrix[dev][i];
    OMPVV_TEST_AND_SET_VERBOSE(errors, (dev * N != sum[dev]));
  }

  omp_set_default_device(def_dev);

  return errors;
}

